opam-version: "2.0"
synopsis: "A template for Ocaml Tooling"
version: "0.3"
maintainer: "msr45@cam.ac.uk"
authors: ["Mukul Rathi"]
homepage: "https://github.com/mukul-rathi/ocaml-testing-template"
