dev-repo: "git+https://github.com/mukul-rathi/ocaml-testing-template.git"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "dune"
  "qcheck"
  "qcheck-alcotest"
  "alcotest"
  "odoc"
  "ocamlformat"
]